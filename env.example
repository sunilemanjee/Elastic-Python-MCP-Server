# Elasticsearch Configuration
ES_URL=your_elasticsearch_url
ES_API_KEY=your_elasticsearch_api_key

# Optional: Username/password authentication (if not using API key)
# ES_USERNAME=your_username
# ES_PASSWORD=your_password

# Optional: CA certificate for SSL verification
# ES_CA_CERT=your_ca_cert_path

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Search Configuration (optional - defaults provided)
PROPERTIES_SEARCH_TEMPLATE=properties-search-template
ELSER_INFERENCE_ID=.elser-2-elasticsearch
ES_INDEX=properties

# Server Configuration (optional - defaults provided)
MCP_HOST=0.0.0.0
MCP_PORT=8001 